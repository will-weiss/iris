{{#text}}
  cursor = {{{text}}};
{{/text}}
{{^text}}
  cursor = {{#keys.length}}context{{/keys.length}}{{^keys.length}}data{{/keys.length}};
  {{#keys}}
    cursor = cursor && cursor["{{{.}}}"];
  {{/keys}}
{{/text}}
{{#variable}}
  cursor = cursor == null ? "" : "" + cursor;
  {{#escaped}}
    cursor = cursor.replace("&", "&amp;").replace('"', "&quot;").replace("<", "&lt;").replace(">", "&gt;");
  {{/escaped}}
{{/variable}}
{{#partial}}
  cursor = typeof {{{name}}} !== "function"
    ? ""
    : {{{name}}}(cursor).split("\n").map(function(s) {
        return "{{{indentation}}}" + s;
      }).join("\n");
{{/partial}}
{{^section}}
  {{{addToResultLeft}}}cursor{{{addToResultRight}}};
{{/section}}
{{#section}}
  {{^inverted}}
    if (cursor) {
      (Array.isArray(cursor) ? cursor : [cursor]).forEach(function(data) {
        var cursor;
        var context = data && typeof data === 'object' ? Object.assign(Object.create(this), data) : this;
        {{#nodes}}
          {{> node}}
        {{/nodes}}
      }, context);
    }
  {{/inverted}}
  {{#inverted}}
    if (!cursor || cursor.length === 0) {
      {{#nodes}}
        {{> node}}
      {{/nodes}}
    }
  {{/inverted}}
{{/section}}